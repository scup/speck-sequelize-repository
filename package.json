{
  "name": "speck-sequelize-repository",
  "version": "0.1.2",
  "description": "Repositories using Speck",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard -v \"src/**/*.js\"",
    "lint:fix": "standard -v --fix \"src/**/*.js\"",
    "lint:watch": "watch -p \"+(src)/**/*.js\" -c \"yarn lint\"",
    "test": "npm run lint && NODE_ENV=test NODE_PATH=./ mocha \"src/**/*.test.js\" --colors --require tests/support.js --reporter spec",
    "test:watch": "npm test -- --watch",
    "coverage": "npm run lint && nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scup/speck-sequelize-repository"
  },
  "keywords": [
    "Javascript",
    "Entities",
    "Repositories"
  ],
  "author": "Scup",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/scup/speck-sequelize-repository/issues"
  },
  "dependencies": {
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "object-mapper": "^5.0.0",
    "pg": "^7.6.1"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.5",
    "mocha": "^5.0.4",
    "moment": "^2.21.0",
    "nyc": "^11.6.0",
    "rosie": "^1.6.0",
    "sequelize": "^4.37.2",
    "sinon": "4.4.6",
    "standard": "^11.0.1",
    "watch-cli": "^0.2.3"
  },
  "peerDependencies": {
    "sequelize": "^3.0.0 || ^4.0.0",
    "object-mapper": "^5.0.0",
    "pg": "^6.0.0 || ^7.6.1"
  },
  "nyc": {
    "branches": 100,
    "check-coverage": true,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  }
}
